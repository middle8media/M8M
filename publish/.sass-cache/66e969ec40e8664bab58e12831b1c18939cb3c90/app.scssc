/* -------------------------------------------- Connect */

#connectTab .quote {
	border:none;
	display: block;
	background: url(../images/seth-avatar.png) no-repeat 3% 50%;
	height: 70px;
	width: 100%;
	-webkit-background-size: auto 90%;
	-moz-background-size: auto 90%;
	background-size: auto 90%;
	cite {
		float: left;
		margin: 25px 0px 0px 70px;
		@include respond-to(medium-screens) { font-size:13px; margin: 27px 0px 20px 63px };
		@include respond-to(portrait-mobile) {margin: 27px 0px 20px 60px };
		@include respond-to(landscape-mobile) {margin: 27px 0px 20px 60px };
	}
}

.name, .email {
	@include respond-to(portrait-mobile) {padding-right: 0px;}
	@include respond-to(landscape-mobile) {padding-right: 0px;}
}

ul.chirp li {
	position: relative;
	list-style: none;
	padding-left: 60px;
}

ul.chirp li img {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 40px;
	height: 40px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	border: none;
}

ul.chirp li .meta, ul.chirp li .meta a {
	font-size: 12px;
}

/* -------------------------------------------- Jot Form */

fieldset {
  border: none;
  padding: 0px;
}

fieldset li {
  list-style: none;
}

.form-required {
  color: #DD4B39;
}

#input_10 {
  margin-bottom: 0px;
}

.six.columns.name, .six.columns.phone, .six.columns.email, .six.columns.website {
  height: 100px;
}

.six.columns.website {
  height: 82px;
}

.twelve.columns.details {
  height: 188px;
}

.twelve.columns.budget {
  height: 100px;
}

#id_10 .form-error-message {
  margin-top: 10px;
}

.form-error-message {
  background: rgba(198, 15, 19, 0.8);
  color: #fff;
  font-size: 13px;
  font-weight: bold;
  margin-bottom: 30px;
  padding: 2px 7px;
}

input[type="text"], input[type="password"], input[type="date"], input[type="datetime"], input[type="email"], input[type="number"], input[type="search"], input[type="tel"], input[type="time"], input[type="url"], textarea {
  margin: 0px;
}

/* -------------------------------------------- Social Networks */

.social-networks {
	span{
	font-size: 60px;
}
	a{
	color: black;
	margin-right: 25px;
	@include respond-to(portrait-mobile) {margin-right: 18px;}
	@include respond-to(landscape-mobile) {margin-right: 99px;}
 	@include respond-to(medium-screens) { margin-right: 0px; }
  	@include respond-to(wide-screens) {}
}
	ul li {
	display: inline;
}
	.facebook a:hover {
	color: #3B5998;
}
	.twitter a:hover{
	color: #00ABF0;
}
	.instagram a:hover {
	color: #845445;
}
}

.social-networks {
	span {
	@include respond-to(medium-screens) { font-size: 52px; }
	}
	a {font-weight: normal;
	}
}


/* -------------------------------------------- Footer */

footer {
	background: none;
	color: #222222;
	border-top: 1px solid #999;
	padding: 17px 0;
	.copywrite {
		text-align: center;
		@include respond-to(medium-screens) {float: right;}
		@include respond-to(wide-screens) {float: right;}
	}
	ul.link-list {
		@include respond-to(portrait-mobile) {margin: 0px -22px 18px; width: 406px;}
		@include respond-to(landscape-mobile) {margin: 0px auto 18px; width: 406px;}
		a {
			font-weight: normal;
		}
		li {
			@include respond-to(portrait-mobile) {margin-left: 19px;}
		}
}
}

.link-list .icon-large {
		font-size: 25px;
		@include respond-to(portrait-mobile) {font-size: 25px;}
		@include respond-to(landscape-mobile) {font-size: 30px;}
}

.site-footer .has-tip {
	border-bottom: none;
}

