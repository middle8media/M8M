@import "compass/reset";
@import "offcanvas";
@import "settings";
@import "foundation";
@import "compass/typography/text/replacement";
@import "compass/css3/transition";
/*@import "compass/layout/sticky-footer";*/
@import "icons";
@import "font-awesome";
@import "general_foundicons";

/* -------------------------------------------- Media Queries */

$break-small: 320px;
$break-medium: 768px;
$break-large: 1024px;

@mixin respond-to($media) {
  @if $media == portrait-mobile {
    @media only screen and (min-width: $break-small) and (max-width: $break-small + 159) { @content; }
  }
  @else if $media == landscape-mobile {
    @media only screen and (min-width: $break-small + 160) and (max-width: $break-medium - 1) { @content; }
  }
  @else if $media == medium-screens {
    @media only screen and (min-width: $break-medium) and (max-width: $break-large - 1) { @content; }
  }
  @else if $media == wide-screens {
    @media only screen and (min-width: $break-large) { @content; }
  }
}

/* -------------------------------------------- General Styles */

/*@include sticky-footer(150px, "#wrapper", "#footer_push", "footer");*/

body {
	font-family: adelle,serif;
	p {
		color: black;
	}
	a {
	cursor: pointer;
	font-weight: bold;
	@include transition-property(all);
  @include transition-duration(.15s);
	}
